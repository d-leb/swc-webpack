{
  "name": "@d-leb/swc-webpack",
  "version": "1.2.12",
  "description": "A library for compiling TypeScript and JavaScript using SWC and Webpack",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d-leb/swc-webpack.git"
  },
  "packageManager": "yarn@3.3.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "defaults",
    "scripts"
  ],
  "bin": "bin/swc-webpack.mjs",
  "scripts": {
    "eslint": "eslint --ext mjs scripts bin --max-warnings=0",
    "eslint:fix": "eslint --ext mjs scripts bin --fix",
    "lint": "yarn eslint && yarn prettier",
    "prettier": "prettier --check 'scripts/**/*.mjs' 'bin/**/*.mjs'",
    "prettier:fix": "prettier --write 'scripts/**/*.mjs' 'bin/**/*.mjs'"
  },
  "dependencies": {
    "@swc/core": "1.3.36",
    "@swc/helpers": "0.4.14",
    "browserslist": "4.21.5",
    "compression-webpack-plugin": "10.0.0",
    "cross-spawn": "7.0.3",
    "dotenv-webpack": "8.0.1",
    "html-webpack-plugin": "5.5.0",
    "imports-loader": "4.0.1",
    "svg-url-loader": "8.0.0",
    "swc-loader": "0.2.3",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-dev-server": "4.11.1"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.4"
  }
}
